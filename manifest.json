{
  "manifest_version": 3,
  "name": "E-Pro",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "toggle-fullscreen": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z",
        "chromeos": "Ctrl+Shift+Z",
        "linux": "Ctrl+Shift+Z"
      },
      "description": "엔트리 작품 실행창 웹페이지 전체화면"
    }
  }
}
